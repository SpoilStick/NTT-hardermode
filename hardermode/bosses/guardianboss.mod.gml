#define guardianboss_create
	var nearestfloor = instance_nearest(argument0,argument1,Floor);
	with(enemy) my_health = 0;
	with(Debris) instance_destroy();
	with(ChestOpen) instance_destroy();
	with(SmallGenerator){
		x=0;
		y=0;
		instance_destroy();
		}
	sound_play(sndCrownCurses);
	with(instance_create(nearestfloor.x+16,nearestfloor.y+16,CustomEnemy)){
		name = "guardianboss";
		maxhealth = 525
		maxhealth += (maxhealth*2/3)*instance_number(Player)
		maxhealth += (maxhealth*2/3)*GameCont.loops
		canfly = true;
		spritealpha = 0;
		size = 3;
		friction = 0.40;
		right = 1;
		
		spr_idle = sprCrownGuardianIdle;
		spr_walk = sprCrownGuardianIdle;
		spr_hurt = sprCrownGuardianHurt;
		spr_dead = sprCrownGuardianDead;
		spr_fire = sprCrownGuardianFire;
		sprite_index = spr_idle;
		mask_index = mskNone;
		
		with(Wall){
			mask_index = mskNone;
		}
		snd_hurt = sndCrownGuardianHurt;
		snd_spc1 = sndCrownGuardianDisappear;
		snd_spc2 = sndGuardianAppear;
		snd_dead = sndCrownGuardianDead;
		
		on_step = script_ref_create(guardianboss_step);
		on_draw = script_ref_create(guardianboss_draw);
		on_hurt = script_ref_create(guardianboss_hurt);
		on_destroy = script_ref_create(guardianboss_dead);
		// Establish center, and then shift position
		_x = x;
		_y = y;
		x = _x + lengthdir_x(128,random(360));
		y = _y + lengthdir_y(128,random(360));
		burst = 0;
		dir = point_direction(x,y,_x,_y);
		spd = 7;
		
		alrm0 = 0;// Alarm to fade into the wall of Guardians. 10sec on-spawn, 7 otherwise
		alrm1 = 30 * 5; // Alarm to fade back in. 5 seconds after fading out.
		alrm2 = 30 * 8; // Shooting alarm. Will adjust accordingly.
		alrm3 = 0;//Death sequence
		deathseq = -1;// Death sequence
		fade = true;// Fade in/Fade out
		
		with(script_bind_draw(guardianwall_draw,-10)){
			_rot = 0;
			_x = other._x;
			_y = other._y;
			owner = other.id;
			alpha = 0;
			spr_ind = sprGuardianIdle;
			name = "guardianwall";
			image_speed = 0.40;
			with(script_bind_draw(guardiandark_draw,-8)){
				owner = other.id;
				alpha = 0;
				x = other._x;
				y = other._y;
			}
		}
			if fork(){
			wait 30
			if instance_exists(self){
				sound_play_music(mus100b)
				if mod_variable_exists("mod","options","intropan") and mod_variable_get("mod","options","intropan") == "1"{
			if UberCont.opt_bossintros == 1{
				mod_script_call("mod", "bossintros", "boss_trigger","GREATER GUARDIAN",global.guardianintrobg,global.guardianintro,global.guardianintrofg,self,60);
				}
			
				
				}
			}
		}
	}
	
#define guardianboss_dead 
	with(Wall){
		mask_index = mskWall;
		if place_meeting(x,y,Player){
			instance_create(x,y,PortalClear);
		}
	}
	with(projectile){
		if team != 2{
			with(instance_create(x,y,ScorpionBulletHit)){
				sprite_index = sprGuardianBulletHit;
			}
			instance_destroy();
		}
	}
	my_health = 0;
	with instance_create(x,y,Portal)
		nompportal = 1
		
#define guardianboss_hurt
	if (deathseq = -1){
		if alrm0 > 5{
			alrm0 -= argument0
			if alrm0 < 5
				alrm0 = 5;
		}
		if alrm2 > 10
			alrm2 -= min(argument0,alrm2 - 1);
		sprite_index = spr_hurt;
		image_index = 0;
		sound_play(snd_hurt);
		
		my_health -= argument0;
		if my_health <= 0{
			my_health = 9999;
			deathseq = 30;
			alrm3 = 60;
			with instances_matching(CustomDraw,"name","guardianwall"){
				spr_ind = sprGuardianHurt;
				image_index = 0;
			}
		}
		
		with(other){
			if "typ" in self{
				if typ = 0 
					other.nexthurt = current_frame + 5;
			}
		}
	}
#define guardianboss_step
	speed = 0;
	if current_frame mod 5 = 0 && random(3) < 1{
		sound_play(choose(sndGuardianAppear,sndGuardianDisappear))
	}
	// Automatically destroys projectiles to keep the stage tidy
	with(projectile){
		if team != 2{
			if point_distance(x,y,other._x,other._y) > 32 * 8{
				instance_destroy();
			}
		}else{
			if point_distance(x,y,other._x,other._y) > 132{
				instance_destroy();
			}
		}
	}
	// Ensures that its own balls get destroyed in pickup range
	with(BigGuardianBullet){
		if point_distance(x,y,other._x,other._y) > 128{
			instance_destroy();
		}
	}
	// Animation handler
	if (image_index + image_speed >= image_number){
		if (deathseq = -1){
			if sprite_index != spr_idle
				sprite_index = spr_idle;
			image_index = 0;
		}else{
			image_index = image_number - 1;
		}
	}
	if spritealpha > 0 && (fade){
		spritealpha -= 0.10;
		if (burst) spritealpha -= 0.20;
		if spritealpha <= 0 && burst = -1 && my_health < maxhealth * 0.33{
			burst = 2;
		}
		mask_index = mskNone;
	}
	if spritealpha < 1 & !(fade){
		spritealpha += 0.10;
		if (burst) spritealpha += 0.20;
		if spritealpha >=1 && burst > 0{
			burst --;
			alrm0 = 1;
		}
		mask_index = mskGuardianIdle;
	}
	// Alarm events. Different alarms are incremented during different states
	if (deathseq = -1){
		if(spd > 0){
			x += lengthdir_x(spd,dir);
			y += lengthdir_y(spd,dir);
			spd *= 0.90;
			if spd < 0.05 spd = 0;
		}
		if (spritealpha = 1){
			x += lengthdir_x(sin(current_frame / 15),dir + (90 * right));
			y += lengthdir_y(cos(current_frame / 20),dir + (90 * right));
		}
		if (alrm0 && !--alrm0){
			fade = true;
			dir = point_direction(_x,_y,x,y);
			spd = 4;
			sound_play(snd_spc1);
			alrm1 = 30;// 5 seconds to fade back in, but factoring in the time to fade out 
		}
		if (alrm1 && !--alrm1){
			var rando = random(360);
			x = _x + lengthdir_x(128,rando);
			y = _y + lengthdir_y(128,rando);
			dir = point_direction(x,y,_x,_y);
			if x > _x right = -1
				else right = 1;
			spd = 4;
			alrm0 = 30 * 4;
			sound_play(snd_spc2);
			alrm2 = round(30 * random_range(1,3));
			if my_health < maxhealth * 0.33{
				alrm0 *= 0.75;
				alrm2 *= 0.50;
			}
			if burst = 0 burst --;
			fade = false;
		}
		if (alrm2 && !--alrm2) && !fade{
			if instance_exists(Player){
				sprite_index = spr_fire;
				image_index = 0;
				var nearest = instance_nearest(x,y,Player);
				with(instance_create(x,y,BigGuardianBullet)){
					team = other.team;
					speed = 8;
					direction = point_direction(x,y,nearest.x,nearest.y);
					hitid = [sprCrownGuardianIdle,"GREATER GUARDIAN"];
					if hspeed < 0 other.right = -1
						else other.right = 1;
				}
				alrm0 += 30;
				alrm2 = max(30,round((30 * random_range(0.5,4)) * max(1 - (GameCont.loops * 0.10),0.33)));
				//trace(alrm2 / 30);
				if my_health < maxhealth * 0.33{
					alrm2 = max(alrm2 * 0.80,10);
				}
			}
		}
	}else{
		if (alrm3 && !--alrm3){
			var wid = sprite_get_width(spr_idle) / 3;
			image_index = 0;
			sound_play(snd_hurt);
			with(instance_create(x+random_range(-wid,wid),y+random_range(-wid,wid),SmallExplosion)){
				hitid = [sprCrownGuardianHurt,"GREATER GUARDIAN"];
				var _dir = point_direction(other.x,other.y,x,y);
				with(instance_create(other.x,other.y,Rad)){
					direction = _dir;
					speed = 3 + random(3);
				}
			}
			with instances_matching(CustomDraw,"name","guardianwall"){
				spr_ind = sprGuardianHurt;
				image_index = 0;
			}
			if (deathseq = 1){
				with(instance_create(x,y,SmallExplosion)){
					hitid = [sprCrownGuardianHurt,"GREATER GUARDIAN"];
				}
				alrm3 = 60;
				deathseq -= 1;
			}else if deathseq = 0{
				with instances_matching(CustomDraw,"name","guardianwall"){
					spr_ind = sprGuardianDead;
					image_index = 0;
				}
				repeat(8){
					with(instance_create(x+random_range(-16,16),y+random_range(-16,16),GreenExplosion)){
						hitid = [sprCrownGuardianHurt,"GREATER GUARDIAN"];
						image_index = 1;
					}
					with(instance_create(x,y,BigRad)){
						speed = 5 + random(3);
						direction = random(360);
					}
					
				}
				repeat(32){
					with(instance_create(x,y,Rad)){
						speed = 7 + random(3);
						direction = random(360);
					}
				}
				with(instance_create(x,y,Corpse)){
					sprite_index = other.spr_dead;
				}
				sound_play(snd_dead);
				instance_destroy();
			}else{
				alrm3 = ceil(deathseq/2);
				deathseq -= 1;
			}
		}
	}
	
	if instance_exists(self){
		with(Player){
			if point_distance(x,y,other._x,other._y) < 256{
				if point_distance(x,y,other._x,other._y) > 132 && nexthurt <= current_frame{
					var _maxspd = maxspeed;
					sprite_index = spr_hurt;
					image_index = 0;
					sound_play(snd_hurt);
					nexthurt = current_frame + 10;
					maxspeed = 6;
					motion_add(point_direction(x,y,other._x,other._y),12);
					if(fork()){
						canwalk = 0;
						wait 5;
						canwalk = 1;
						maxspeed = _maxspd;
					}
				}
			}else{
				if (canwalk){
					canwalk = false;
					with(script_bind_draw(correctcoords,-10)){
						sprite_index = sprGuardianDead;
						image_index = image_number;
						image_speed = -0.40;
						queuedestroy = 0;
						alrm0 = 60;
						slave = other.id;
						x = slave.x;
						y = slave.y;
					}
				}
			}
		}
	}
	
#define correctcoords
	if !instance_exists(slave){
		if alrm0 > 1 alrm0 = 1;
	}else{
		slave.x = x;
		slave.y = y;
	}
	if image_index + image_speed < 0{
		image_speed = 0.40;
		image_index = 0;
		sprite_index = sprGuardianIdle;
	}
	if image_index + image_speed >= image_number && sprite_index = sprGuardianDead queuedestroy = 1;
	if (alrm0 && !--alrm0){
		image_speed = 0.40;
		image_index = 0;
		sprite_index = sprGuardianDead;
		with(instances_matching(CustomDraw,"name","guardianwall")){
			other.x = _x;
			other.y = _y;
		}
	}
	draw_sprite(sprite_index,image_index,x,y);
	if (queuedestroy){
		if instance_exists(slave){
			slave.canwalk = true;
		}
		instance_destroy();
	}
#define guardiandark_draw
	with(projectile) if depth > -10 depth = -10;
	with(Explosion) if depth > -10depth = -11;
	with(Pickup) if depth > -10 depth = -10;
	with(Effect) if depth > -10 depth = -10;
	with(CrystalShield) if depth > -10 depth = -10;
	with(CrystalShieldDisappear) if depth > -10 depth = -10;
	with(RogueStrike) if depth > -9 depth = -9;
	draw_set_alpha(alpha);
	draw_set_color(c_black);
	draw_rectangle(view_xview,view_yview,view_xview+game_width,view_yview+game_height,0);
	draw_set_alpha(1);
	draw_set_color(c_white);
	with(hitme){
		event_perform(ev_draw,0);
	}
	if instance_exists(owner){
		if alpha < 1 alpha += 0.05;
		if alpha > 0.5 && TopCont.darkness = 0
			TopCont.darkness = 1;
	}else{
		alpha -= 0.05;
		if alpha <= 0.5{
			TopCont.darkness = 0;
			with(Player){
				if !place_meeting(x,y,Floor){
					var nearest = instance_nearest(x,y,Floor);
					x = nearest.x+16;
					y = nearest.y+16;
					instance_create(x,y,PortalClear);
					//instance_create(x,y,CrystalShieldDisappear);
				}
				if place_meeting(x,y,Wall){
					instance_create(x,y,PortalClear);
				}
			}
		}
		if alpha <= 0 instance_destroy();
	}
#define guardianboss_draw 
	draw_sprite_ext(sprite_index,image_index,x,y,right,1,0,c_white,spritealpha);
	//draw_circle(_x,_y,5,1);
	
#define guardianwall_draw
	var _amount,_len,_dir;
	_amount = 24;
	_len = 96;
	_dir = 360/_amount;
	if (spr_ind = sprGuardianIdle)
		_rot += 0.75
	for(var i = 0;i<_amount;i++){
		var _xsc = 1;
		// Outermost, slowest rotate 
		for(var o = 3;o>=1;o--){
			var _X = _x - lengthdir_x(_len*(1 + (0.50*o)),(_dir*i)+(_rot * (1/o)));
			var _Y = _y + lengthdir_y(_len*(1 + (0.50*o)),(_dir*i)+(_rot * (1/o)));
			if _X > _x _xsc = -1;
				else _xsc = 1;
			var img_ind;
			if spr_ind != sprGuardianIdle{
				img_ind = median(0,image_index,sprite_get_number(spr_ind)-1);
			}else{
				img_ind = ((i * _amount) + (current_frame * 0.40)) mod sprite_get_number(spr_ind);
			}
			draw_sprite_ext(spr_ind,img_ind,_X,_Y,_xsc,1,0,c_white,alpha);
			if spr_ind = sprGuardianIdle{
				if current_frame mod 10 = 0 && random(250 - (5 * GameCont.loops)) < 1{//random(800 - (GameCont.loops * 50)) < 1{
					for(var i = -1;i<=1;i++){
						with(instance_create(_X,_Y,GuardianBullet)){
							team = 1;
							speed = 1.5;
							damage = 3;
							hitid = [sprGuardianIdle,"LESSER GUARDIAN"];
							sound_play(sndGuardianFire)
							if !instance_exists(Player){
								direction = point_direction(x,y,other._x,other._y) + (15 * i);
							}else{
								var nearest = instance_nearest(x,y,Player);
								direction = point_direction(x,y,nearest.x,nearest.y) + (15 * i);
							}
						}
					}
				}
			}
		}
	}
	if instance_exists(owner){
		if alpha < 1 alpha += 0.125;
