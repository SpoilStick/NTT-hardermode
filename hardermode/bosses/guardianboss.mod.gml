#define GuardianStatue_create(_x, _y)

// /gml mod_script_call("mod", "customenemies", "GuardianStatue_create", mouse_x, mouse_y)

with instance_create(_x, _y-4, CustomObject){
	name = "guardianstatue"
	spr_idle1 = global.Statue1
	spr_idle2 = global.Statue2
	spr_idle3 = global.Statue3
	image_angle = 45
	image_angle1 = image_angle
	image_angle2 = image_angle
	image_angle3 = image_angle
	image_angle4 = image_angle
	mask_index = mskLaserCrystal
	hitid = [sprThroneStatue,"HOW DID THIS KILL YOU?"]
	spr_shadow = shd32
	size = 2
	canfly = 1
	team = 1
	z = 0
	rot = 0
	rdir = 1
	image_alpha = 0;
	on_step = script_ref_create(GuardianStatue_step)
	
	with script_bind_draw(GuardianStatue_draw, -10){
		zscale = 1
		creator = other
		image_angle = other.image_angle
		image_angle1 = other.image_angle
		image_angle2 = other.image_angle
		image_angle3 = other.image_angle
		image_angle4 = other.image_angle
		image_xscale = other.image_xscale
		image_yscale = other.image_yscale
		image_speed = other.image_speed
		}
	
	return id;
	}
