// { Sprites
global.gatorbossidle = sprite_add_base64("", 15, 16,16)
global.gatorbosswalk = sprite_add_base64("", 6, 18,18)
global.gatorbosshurt = sprite_add_base64("", 3, 18,18)
global.gatorbossdead = sprite_add_base64("", 6, 18,18)
global.gatorbosscharge = sprite_add_base64("",7,40,16)
global.gatorbosschargehurt = sprite_add_base64("",3,40,16)
global.gatorbosspunch = sprite_add_base64("",10,40,16)
global.punch = sprite_add_base64(
"",2,16,16)
global.gatorintro = sprite_add_base64("",1,246,0 )
global.gatorintrofg = sprite_add_base64("",1,246,0 )
global.gatorintrobg = sprite_add_base64("",1,246,0 )
//}
#define draw 
	//with instances_matching_gt(Gator,"z",0)
