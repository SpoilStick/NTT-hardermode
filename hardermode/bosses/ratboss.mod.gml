//{ Sprites
global.ratbossidle = sprite_add_base64("", 6, 44,70)
global.ratbossinvulnidle = sprite_add_base64("", 6, 44,70)
global.ratbossfire = sprite_add_base64("", 6, 44,70)
global.ratbosshurt = sprite_add_base64("", 3, 44,70)
global.ratbossdead = sprite_add_base64("", 9, 44,70)
global.ratbossappear = sprite_add_base64("", 16, 44,70)
global.ratbossdisappear = sprite_add_base64("", 12, 44,70)

global.tenticleappear = sprite_add_base64("", 21, 12,12)
global.tenticleidle = sprite_add_base64("", 8, 12,12)
global.tenticlehurt = sprite_add_base64("", 3, 12,12)
global.tenticledead = sprite_add_base64("", 6, 12,12)
//}
//{ RatBoss
#define ratbosssleep_create(_x,_y)
with instance_create(_x,_y,CustomEnemy){
	name = "ratbosssleep"
	on_step = script_ref_create(ratbosssleep_step)
	on_destroy =  script_ref_create(ratbosssleep_dead)
	team = 1
	maxhealth = 50
	sleeping = 1; // joao platformer
	mask_index = mskBandit
	return id;
	}
#define ratbosssleep_dead
ratboss_create(x,y)
#define ratboss_create(_x,_y)
with instance_create(_x,_y,CustomEnemy){
	name = "ratboss"
	boss = 1
	team = 2
	hitid = [global.ratbossidle,"Mutant Rat"]
	spr_idle = global.ratbossappear
	spr_dead = global.ratbossdead
	snd_hurt = sndOasisBossHurt
	maxhealth = 600 + (300*instance_number(Player))*(0.5*GameCont.loops)
	my_health = maxhealth
	storehp = maxhealth
	midhealth = maxhealth/2
	direction = random(360)
	spr_shadow = -4
	size = 3
	mask_index = mskVan
	friction = 32
	raddrop = 75
	canmelee = 0
	meleedamage = 3
	target = 0
	targetvisible = 0
	alrm1 = 100 //fire
	walk = 0
	alrm2 = 0 //mad
	ax = x
	ay = y
	tenticles = 0
	underwater = 0
	sleeping = 1; // joao platformer
	on_step = script_ref_create(ratboss_step)
	on_death = ratboss_dead
	return id;
	}
#define ratboss_step
if spr_idle == global.ratbossappear && image_index >= 15{
	spr_idle = global.ratbossinvulnidle
	canmelee = 1
	team = 1
	sound_play_pitch(sndOasisBossFire,2)
	}

if(nexthurt > current_frame) && spr_idle == global.ratbossidle{
	if(sprite_index != spr_hurt){
		image_index = 0;
		sprite_index = spr_hurt;
		}
	if my_health < midhealth{
		midhealth = -1
		canmelee = 0
		spr_idle = global.ratbossdisappear
		underwater = random_range(25,100)
		team = 2
		}
	}
else{
	sprite_index = spr_idle;
	}

if(instance_exists(Player))
	target = instance_nearest(x,y,Player);
else
	target = 0;
	
if(instance_exists(target) && collision_line(x,y,target.x,target.y,Wall,0,0) < 0)
	targetvisible = 1;
else
	targetvisible = 0;	
if(target > 0){
	if(targetvisible){
		var p = instance_nearest(x,y,Player)
		alrm1 -= 1
		gunangle = point_direction(x,y,p.x,p.y)
		if(target.x > x)  
			image_xscale = 1
		else
			image_xscale = -1
		}
	}
if team = 2	
with instances_matching_ne(projectile,"team",0) if place_meeting(x,y,other){
		sound_play(sndCrystalRicochet);
		with instance_create(x,y,Deflect)
			image_angle = other.direction
		instance_destroy()
		}	

if tenticles == 0{
	if spr_idle == global.ratbossappear{
		repeat 2{
		var r = random_range(40,80);
		var r2 = random(360);
		tenticle_create(x+lengthdir_x(r,r2),y+lengthdir_y(r,r2),self)
		tenticles += 1
		}
		}
	if spr_idle == global.ratbossinvulnidle && alrm2 <= 0{
		alrm2 = 200
		spr_idle = global.ratbossidle
		}
	}
with instances_matching(CustomEnemy,"name","tenticle") if place_meeting(x,y,other){
	motion_add(point_direction(other.x,other.y,x,y),14)
	}

if spr_idle == global.ratbossidle && alrm2 <= 0{
	canmelee = 0
	spr_idle = global.ratbossdisappear
	underwater = random_range(25,100)
	team = 2
	}
if underwater > 0
	underwater -= 1
	
if spr_idle == global.ratbossdisappear && image_index >= 11
	spr_idle = -4
	
if underwater <= 0 && spr_idle == -4
	spr_idle = global.ratbossappear
	
if spr_idle != (global.ratbossappear) && underwater <= 0{
	if alrm2 > 0{
		alrm2 -= 1
		storehp = my_health
		spr_hurt = global.ratbosshurt
		snd_hurt = sndOasisBossHurt
		x = ax+random_range(-2,2)
		y = ay+random_range(-2,2)
		}
	if alrm2 <= 0{
		my_health = storehp
		spr_hurt = global.ratbossinvulnidle
		snd_hurt = -4
		x = ax
		y = ay
		}
	if alrm1 > 0{
		alrm1 -= 1
		}
	if alrm1 <= 0 && targetvisible{
		if alrm2 <= 0 && team = 1{
			alrm1 = 300
			var a = 0;
			var b = 0;
			var c = choose(-1,1);
			var n = choose(3,4,5,6);
			repeat(24){
				repeat(n){
				if instance_exists(self) && alrm2 <= 0 && (spr_idle != global.ratbossappear and spr_idle != global.ratbossdisappear and spr_idle != -4)
				with instance_create(x,y,EnemyBullet1){
					sprite_index = sprFireBall
					hitid = [global.ratbossidle,"Mutant Rat"]
					speed = 4.5
					team = 1
					direction = a + b
					creator = other
					image_angle = direction
					a += 360/n
					}
				}
				b += 12*c +b/n*2
				wait(4)
				}
			}
		else{
			if instance_exists(self) && team = 1{
				alrm1 = 10
				var a = point_direction(x,y,target.x,target.y)
				with instance_create(x,y,EnemyBullet1){
					sprite_index = sprFireBall
					hitid = [global.ratbossidle,"Mutant Rat"]
					speed = 5
					team = 1
					direction = a + random_range(-50,50)
					creator = other
					image_angle = direction
					}
				}
			}
		}
	}
else{	
	x = ax
	y = ay
	my_health = storehp
	sprite_index = spr_idle
	}
#define ratboss_dead
with instance_create(x+16,y+16,Effect){
	sprite_index = sprFloor103
	image_speed = 0
	image_index = 1
	depth = -1
	}
with instance_create(x+16,y+16,CustomObject){
		name = "VenuzElevator"
		image_speed=0
		sprite_index = sprFloor103
		image_alpha = 0
		image_index = 1
		depth = 7
		activated = 1
		}
#define ratbosssleep_step
if distance_to_object(Player)<120
	instance_destroy()
#define tenticle_create(_x,_y,cre)
with(instance_create(_x,_y,CustomEnemy)){
	team = 1
	name = "tenticle"
	maxhealth = 50
	speed = 0
	friction = 0.4
	canmelee = 0
	creator = cre
	gunangle = random(360)
	mask_index = mskBandit
	hitid = [global.tenticleidle,"Rat Tenticle"]
	alrm1 = random_range(60,100) //fire
	spr_idle = global.tenticleappear
	spr_walk = global.tenticleidle
	spr_hurt = global.tenticlehurt
	spr_dead = global.tenticledead
	sleeping = 1; // joao platformer
	on_step = script_ref_create(tenticle_step)
	on_destroy = script_ref_create(tenticle_dead)
	return id;
	}
#define tenticle_step
if spr_idle == global.tenticleappear && image_index >= 20{
	spr_idle = global.tenticleidle
	sound_play_pitch(sndTurtleHurt,1.3)
	}

if spr_idle = global.tenticleidle{
	if(nexthurt > current_frame){
			if(sprite_index != spr_hurt) {
				image_index = 0;
				sprite_index = spr_hurt;
				}
			}
		else{
			if(speed > friction) sprite_index = spr_walk;
			else sprite_index = spr_idle;
			}
	}
else
	sprite_index = spr_idle 
	
if instance_exists(Player) and sprite_index = spr_idle{
target = instance_nearest(x,y,Player)
gunangle = point_direction(x,y,target.x,target.y)
}
else target = -1

if target != -1{
	motion_add(gunangle,0.5)
	if alrm1 > 0
		alrm1--
	else if alrm1 != -1{
		alrm1 += 100
		sound_play_pitch(sndJockFire,1.3)
		motion_set(gunangle-180,5)
		with instance_create(x,y,JockRocket){	
			hitid = other.hitid
			team = other.team
			creator = other
			damage=1
			image_xscale *= 0.5
			direction = other.gunangle
			image_angle = direction
			}
		}
	}
	
if speed > 2 
speed = 1
#define tenticle_dead
with instances_matching(CustomEnemy,"name","ratboss")
	tenticles -= 1
//}
