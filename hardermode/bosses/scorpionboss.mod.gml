#define fancyscorpionboss_death
//by 9joao6
if(fork())
{
	var xd_ = x;
	var yd_ = y;
	var fl_ = instance_nearest(xd_,yd_,Floor);
	
	wait(1);
	
	if instance_exists(Player)
		with fl_
