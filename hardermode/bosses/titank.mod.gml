#define scrNuke(z, size)
// THE GODDESS OF EXPLOSIONS
//     SMILES UPON YOU
with(instance_create(x, y-z, Explosion))
{
	team = 2;
	visible = 0;
	image_xscale *= 2.5*size;
	image_yscale = image_xscale;
	
	with(hitme)
	{
		if("team" in self
		&& team != other.team
		&& distance_to_object(other) <= 0)
		{
			my_health -= 20+10*GameCont.level;
		}
	}
}
// ground ring
for(var i = 0; i < 360; i += 60/size)
{
	with(instance_create(x, y-z, Explosion))
	{
		team = other.team;
		mask_index = mskNone;
		direction = i;
		speed = 8*size/1.5;
		vspeed *= 0.75;
	}
}
// tiny ground ring
for(var i = 15; i < 360; i += 60/size)
{
	with(instance_create(x, y-z, SmallExplosion))
	{
		team = other.team;
		mask_index = mskNone;
		direction = i;
		speed = 10*size/1.5;
		vspeed *= 0.75;
	}
}
// pillar
repeat(32*size)
{
	with(instance_create(x+lengthdir_x(random(8)*size, random(360)), y+lengthdir_y(random(8)*size, random(360))-z, SmallExplosion))
	{
		team = other.team;
		mask_index = mskNone;
		vspeed = -random(4)*size;
		var d = depth;
		
		if(fork())
		{
			while(instance_exists(self))
			{
				depth = d-(y+ystart)/2;
				wait(1);
			}
			exit;
		}
	}
}
// top ring
for(var i = 0; i < 360; i += 24/size)
{
	with(instance_create(x, y-z, Explosion))
	{
		team = other.team;
		mask_index = mskNone;
		direction = i;
		speed = 6*size;
		vspeed *= 0.5;
		vspeed -= 6*size;
		image_xscale = 1.5;
		image_yscale = image_xscale;
		var d = depth;
		
		if(fork())
		{
			while(instance_exists(self))
			{
				depth = d-(y+ystart);
				wait(1);
			}
			exit;
		}
	}
}
