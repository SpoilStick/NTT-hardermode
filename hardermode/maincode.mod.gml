		}
	with FloorMaker{ //more directional levels
		if ("pick" not in self){ 
			pick = choose(0,90,180,270);
			}
		if random(3)>2
			direction = pick
		}
	with GenCont if GameCont.subarea == 2 and instance_number(Floor) >= 100 and Player.my_health == Player.maxhealth{ // fishskull spawner, sometimes puts it outside but on_step puts it back inside, shrug
		var ch_ = 0;
		with instances_matching(CustomHitme,"name","fishskull")
			ch_ += 1
		wait 1
		if ch_ == 0{	
			var flr_ = instance_furthest(Player.x,Player.y,Floor);
			with instance_nearest(flr_.x+random_range(-112,112),flr_.y+random_range(-112,112),Floor)
			mod_script_call("mod", "props", "fishskull_create", x+16, y+16)

			}
