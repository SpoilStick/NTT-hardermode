								}else
								if UberCont.opt_bossintros == 1
									mod_script_call("mod", "bossintros", "boss_trigger", "BIG BANDIT", sprBossIntroBackLayer,sprBossIntro,mskNone,instance_nearest(x,y,Player),0)
