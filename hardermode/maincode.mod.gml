switch(GameCont.area){
case 1:						// { desert, first 3 levels
	if global.venuz = 1 and instance_exists(Wall){
	with instance_nearest(10016,10016,Player)
	mod_script_call("mod", "props", "VenuzCar_create", y, x,50)
	sound_loop(sndCarLoop)
	global.venuz = 0
	}
	
	
	if ("popoplease" not in self) {
		if GameCont.loops > 0{
			var i = 0;
			repeat(GameCont.loops){
				mod_script_call("mod", "enemies", "makeavan_create", i);
				i+=(100+random(100))
				}
			}
		var j = 0;
		repeat(GameCont.loops + 1) {
			mod_script_call("mod", "enemies", "makeapopo_create", i);
			j+=(100+random(100))
		}
		popoplease = 1
	}
	
	with GenCont if "settip" not in self{
		if random(10)<1 and global.runcount < 5
			tip = "                     click @W[modded]@s to#                  hide the list of mods"
		else if random(10)<1
			tip = "monsters absorb radiation"
		settip = 1
		}
	with Bones if "randomchance" not in self{
		if random(320)<1{ //cactus party! \o/
			with Floor if random(5)<4
				instance_create(x+16,y+16,Cactus)
			}
		with Floor if random(150)<1{ //hoodoos ahoy
			instance_create(x,y,TopDecalDesert)
			}
		randomchance = 1
		}
	with TopDecalDesert if "why_are_you_so_far_down" not in self{// hoodoos dont look 3d when they are so low when at the bottom of a wall, the base of the hoodoo is farther from the wall than when it is above a wall, blame jw
		if place_meeting(x,y-16,Wall)
			y-=8
