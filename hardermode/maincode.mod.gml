#define move_player_snap(_index, _x, _y)
    with(player_find(_index)){
        var _vo = view_object[_index];
            _dis = point_distance(_x, _y, x, y) * (7 / 3);
            _dir = point_direction(_x, _y, x, y) + 180;

         // Move Player:
        x = _x;
        y = _y;

         // Snap Camera:
        if(fork()){
            with(instance_create(x + lengthdir_x(_dis, _dir), y + lengthdir_y(_dis, _dir), GameObject)){
                view_object[_index] = id;
                wait 1;
                if(instance_exists(self)) instance_destroy();
            }
            view_object[_index] = _vo;
            exit;
        }
    }
