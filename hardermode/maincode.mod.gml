		with IDPDBullet
			speed=2
		with instances_matching_ne(AllyBullet,"reset",1) if team = 0{
			reset = 1
			speed=1+choose(0,0,1)
			}
		}
	with SuperFrog{
		instance_change(Bubble,1)
		instance_destroy()
		}
	with Lightning{
		if instance_exists(creator){
			team = 0
			x=creator.x
			y=creator.y
			damage = 3
			force = random(360)
			hitid = [sprLightningDeath,"PHYSICS"]
			}
		}
	with instances_matching(Effect,"sprite_index",sprPlayerBubble){
		if instance_exists(creator){
			x= creator.x+creator.hspeed
			y= creator.y+creator.vspeed
			}
		else{
			sprite_index = sprPlayerBubblePop
			image_speed = 0.4
			
			}
		}
	with instances_matching(Effect,"sprite_index",sprPlayerBubblePop) if image_index >= 3
		instance_destroy()
	break; //}
case "jungle":				// { jungle
	if random(12)<1 
	with instance_create(view_xview+random(game_width*2)-game_width/2,view_yview+random(game_height*2)-game_height/2,SnowFlake){
		sprite_index = sprLeaf
		}
	with enemy if instance_exists(target){
		if (target>=0 && collision_line(x,y,target.x,target.y,Wall,0,0) < 0)
			targetvisible = 1;
			else 
			targetvisible = 0;	
		if targetvisible == 0 and image_alpha > 0
			image_alpha -= 0.1
		if targetvisible == 1
			image_alpha = 2
		if image_alpha <=0 and speed > friction if random(5)<1
			with instance_create(x,y,Feather)
				sprite_index = sprLeaf
		}	
	break; //}
