if mod_variable_exists("mod","options","rads") and mod_variable_get("mod","options","rads") == "1"{
	with instances_matching_ne(enemy,"team",3){
		with Rad if distance_to_object(other)< 32 and distance_to_object(Player)>32{
			motion_add(point_direction(x,y,other.x,other.y),0.5)
