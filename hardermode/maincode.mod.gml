//{ fancy portals
with Player if "scale" not in self
	scale = 1
if mod_variable_exists("mod","options","portals") and mod_variable_get("mod","options","portals") == "1" {
	with Portal if "nompportal" not in self and !instance_exists(BigPortal){
		image_alpha = 0
		with Player{
			if "scale" not in self
					scale = 1
			var por = instance_nearest(x,y,Portal)
			if point_distance(por.x,por.y,x,y)>24{
				por.endgame = 100
				//if GameCont.area == "cavesinv"{
					if image_alpha = 0{
					scale = 1
					if "swimming" in self and swimming = 1
						image_alpha = 0
					else if mask_index != mskNone
						image_alpha = 1
					//mask_index = mskPlayer
					can_shoot = 1
					if race = "steroids"
						bcan_shoot = 1
					canwalk = 1
					reload = 0
					}
				}
			else{
				por.endgame -= 1/instance_number(Player)
				
				if scale > 0
					scale -= 0.03
				with script_bind_draw(PortalShrink,-10)
					creator = other
				image_alpha = 0
				//can_shoot = 0
				//bcan_shoot = 0
				canwalk = 0
				if distance_to_point(other.x,other.y)>2
				direction = point_direction(x,y,other.x,other.y)
				else speed = 0	
				
				}
			}
			with script_bind_draw(StablePortal,-10)
					creator = other
		}
	
	with GenCont// if GameCont.area != "throne"
		with Player if "scale" in self and !instance_exists(Portal){
			if "scale" not in self
					scale = 1
			scale = 1
			image_alpha = 1
			can_shoot = 1
			if race = "steroids"
				bcan_shoot = 1
			canwalk = 1
			reload = 0
			}
	}//}
