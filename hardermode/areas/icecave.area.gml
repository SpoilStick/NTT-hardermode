global.trans	 = sprite_add_base64("",8,0,0)
global.bot		 = sprite_add_base64("",6,0,0)
global.decal	 = sprite_add_base64("",2,16,24)
global.sprCrystalPropBlueDead = sprite_add_base64("",4,12,12)
global.sprCrystalPropGreenDead = sprite_add_base64("",4,12,12)

#define area_name
return "4-?";

#define area_sprite(q)
switch (q) {
    case sprFloor1: return sprFloor5B;
    case sprFloor1B: return sprFloor5B;
    case sprFloor1Explo: return sprFloor5Explo;
    case sprWall1Trans: return sprWall5Trans;
    case sprWall1Bot: return global.bot;
    case sprWall1Out: return sprWall5Out;
    case sprWall1Top: return sprWall5Top;
    case sprDebris1: return sprDebris5;
	case sprDetail1: return sprDetail5;
}

#define area_transit
if (lastarea != "icecave" && area == "cavesecret") {
    area = "icecave";
	}

#define area_finish
	area = "city";
	subarea = 1;
	sprite_restore(sprSpiderIdle);
	sprite_restore(sprSpiderWalk);
	sprite_restore(sprSpiderHurt);
	sprite_restore(sprSpiderDead);
#define area_setup
sound_stop_all()
sound_play_music(mus100)
goal = 145;
background_color = make_color_rgb(180, 189, 197);
BackCont.shadcol = 786950;
TopCont.darkness = 1;


#define area_make_floor
instance_create(x, y, Floor)
var turn = choose(0,0,0,0,90,90,90,-90,-90,-90,180)
direction += turn;
if (turn == 180 && point_distance(x, y, 10016, 10016) > 48) {
    // turnarounds - weapon chests spawn in such
    instance_create(x, y, Floor);
    instance_create(x + 16, y + 16, WeaponChest);
}
if (random(19 + instance_number(FloorMaker)) > 22) {
    // dead ends - ammo chests spawn in such
    if (point_distance(x, y, 10016, 10016) > 48) {
        instance_create(x + 16, y + 16, AmmoChest);
        instance_create(x, y, Floor);
    }
    instance_destroy();
} else if (random(16) < 1) {
    // branching
    instance_create(x, y, FloorMaker);
}
#define area_start
with enemy friction = 0.1
with Wall if random(6)<1{ 
	if place_meeting(x+16,y,Floor)
		with instance_create(x+16,y,Bones){
			sprite_index = global.decal
			image_xscale = 1
			with instance_create(x,y,CrystalProp){
				image_alpha = 0
				mask_index = mskNone
				spr_shadow = mskNone
				creator = other
				}
			}
	else if place_meeting(x-16,y,Floor)
		with instance_create(x,y,Bones){
			sprite_index = global.decal
			image_xscale = -1
			with instance_create(x,y,CrystalProp){
				image_alpha = 0
				mask_index = mskNone
				spr_shadow = mskNone
				creator = other
				}
			}
	}

with Floor{
	traction = 0.1
	}
sprite_replace_base64(sprSpiderIdle,"",8,12,12 )
sprite_replace_base64(sprSpiderWalk,"",6,12,12 )
sprite_replace_base64(sprSpiderHurt,"",3,12,12 )
sprite_replace_base64(sprSpiderDead,"",6,12,12 )
#define area_pop_enemies
if (random(3) < 1) with instance_create(x + 16, y + 16, Spider)
	raddrop += 50
if (random(24) < 1) instance_create(x + 16, y + 16, LaserCrystal)
if (random(11) < 1) with instance_create(x + 16, y + 16, LightningCrystal)
						spr_fire = sprLaserCrystalFire
if (random(11) < 1) mod_script_call("mod", "enemies", "plasmacrystal_create", x + 16, y + 16);
if (random(48) < 1) repeat 10 instance_create(x + 16, y + 16, Wolf)
if (random(6) < 1) mod_script_call("mod", "enemies", "bigcrystal_create", x + 16, y + 16);
if (random(24) < 1) mod_script_call("mod", "enemies", "EliteSpider_create", x + 16, y + 16);
#define area_pop_props
if (random(4) < 1)with instance_create(x + 16, y + 16, CrystalProp){
	if (random(2)<1){
		spr_idle = sprCrystalPropBlue
		spr_hurt = sprCrystalPropBlueHurt
		spr_dead = global.sprCrystalPropBlueDead
		}
	else{
		spr_idle = sprCrystalPropGreen
		spr_hurt = sprCrystalPropGreenHurt
		spr_dead = global.sprCrystalPropGreenDead
		}
	}
if (random(12) < 1) with instance_create(x + 16, y + 16, Hydrant){
		spr_idle = sprIcicle
		spr_hurt = sprIcicleHurt
		spr_dead = sprIcicleDead
		}

#define area_mapdata(lx, ly, lp, ls, ws, ll)
return [lx, 0];